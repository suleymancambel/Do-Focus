<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.norfsoftware.dofocus">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Notes">
        <activity
            android:name="com.norfsoftware.dofocus.mola"
            android:exported="false"
            android:screenOrientation="fullSensor"/>
        <activity
            android:name="com.norfsoftware.dofocus.Start_activity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.norfsoftware.dofocus.Activity.UpdateNotesActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.norfsoftware.dofocus.Activity.InsertNotesActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.norfsoftware.dofocus.MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.norfsoftware.dofocus.pomodoro_sayac"
            android:exported="true"
            android:screenOrientation="sensor" />
        <activity
            android:name="com.norfsoftware.dofocus.progresstart"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.norfsoftware.dofocus.Activity.SplashScreen"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/adsname"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>

</manifest>